          "tags": ["general", "enhanced", "high-performance"],
          "cost_per_1k_tokens": 0.00012,
          "languages": ["multilingual"],
          "use_cases": ["high_accuracy", "general_purpose", "rag"]
        },
        "voyage-finance-2": {
          "name": "voyage-finance-2",
          "dimension": 1024,
          "description": "Specialized model for financial documents",
          "max_sequence_length": 32000,
          "recommended": false,
          "tags": ["finance", "specialized", "domain-specific"],
          "cost_per_1k_tokens": 0.00012,
          "languages": ["en"],
          "use_cases": ["financial_analysis", "specialized_finance"]
        },
        "voyage-law-2": {
          "name": "voyage-law-2",
          "dimension": 1024,
          "description": "Specialized model for legal documents",
          "max_sequence_length": 32000,
          "recommended": false,
          "tags": ["legal", "specialized", "domain-specific"],
          "cost_per_1k_tokens": 0.00012,
          "languages": ["en"],
          "use_cases": ["legal_analysis", "specialized_legal"]
