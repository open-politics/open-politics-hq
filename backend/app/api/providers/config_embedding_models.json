          "cost_per_1k_tokens": 0.00012,
          "languages": ["code"],
          "use_cases": ["code_search", "technical_documentation"]
        }
      }
    }
  },
  "default_providers": {
    "development": "ollama",
    "production": "jina"
  },
  "recommended_models": {
    "general_purpose": ["nomic-embed-text", "voyage-3", "jina-embeddings-v2-base-en", "bge-base"],
    "high_performance": ["voyage-3.5", "jina-embeddings-v3", "bge-large", "text-embedding-ada-002"],
    "fast_inference": ["all-minilm", "voyage-3-lite", "jina-embeddings-v2-small-en"],
    "multilingual": ["voyage-3", "bge-large", "jina-embeddings-v3", "text-embedding-ada-002"],
    "cost_effective": ["nomic-embed-text", "voyage-3-lite", "jina-embeddings-v2-small-en", "text-embedding-3-small"],
    "specialized": ["voyage-finance-2", "voyage-law-2", "voyage-code-2"]
  }
} 